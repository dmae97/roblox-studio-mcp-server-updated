{\n  \"name\": \"roblox-studio-mcp-server\",\n  \"version\": \"1.0.0\",\n  \"description\": \"Roblox Studio Model Context Protocol(MCP) Server implementation with TypeScript\",\n  \"main\": \"dist/index.js\",\n  \"type\": \"module\",\n  \"scripts\": {\n    \"build\": \"tsc\",\n    \"start\": \"node dist/index.js\",\n    \"dev\": \"nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts\",\n    \"lint\": \"eslint . --ext .ts\",\n    \"lint:fix\": \"eslint . --ext .ts --fix\",\n    \"test\": \"jest\",\n    \"test:coverage\": \"jest --coverage\",\n    \"test:integration\": \"jest --config jest.integration.config.js\",\n    \"test:security\": \"npm test -- --testPathPattern=\\\"security\\\"\",\n    \"test:performance\": \"k6 run performance/k6-load-test.js\",\n    \"test:websockets\": \"k6 run performance/socket-test.js\",\n    \"docker:build\": \"docker build -t roblox-studio-mcp-server .\",\n    \"docker:run\": \"docker run -p 3000:3000 -v .env:/app/.env roblox-studio-mcp-server\",\n    \"docs\": \"jsdoc -c jsdoc.json\",\n    \"docs:dev\": \"docsify serve docs\",\n    \"audit\": \"npm audit --production\",\n    \"audit:fix\": \"npm audit fix --production\",\n    \"security:check\": \"npm run audit && npm run snyk\"\n  },\n  \"keywords\": [\n    \"roblox\",\n    \"studio\",\n    \"mcp\",\n    \"model-context-protocol\",\n    \"typescript\"\n  ],\n  \"author\": \"\",\n  \"license\": \"MIT\",\n  \"dependencies\": {\n    \"@modelcontextprotocol/sdk\": \"^1.0.0\",\n    \"bcrypt\": \"^5.1.1\",\n    \"cors\": \"^2.8.5\",\n    \"dotenv\": \"^16.3.1\",\n    \"express\": \"^4.18.2\",\n    \"helmet\": \"^7.1.0\",\n    \"jsonwebtoken\": \"^9.0.2\",\n    \"node-cache\": \"^5.1.2\",\n    \"rate-limiter-flexible\": \"^2.4.2\",\n    \"swagger-jsdoc\": \"^6.2.8\",\n    \"swagger-ui-express\": \"^5.0.0\",\n    \"uuid\": \"^9.0.1\",\n    \"winston\": \"^3.11.0\",\n    \"ws\": \"^8.15.1\"\n  },\n  \"devDependencies\": {\n    \"@types/bcrypt\": \"^5.0.2\",\n    \"@types/cors\": \"^2.8.17\",\n    \"@types/express\": \"^4.17.21\",\n    \"@types/jest\": \"^29.5.11\",\n    \"@types/jsonwebtoken\": \"^9.0.5\",\n    \"@types/node\": \"^20.10.4\",\n    \"@types/supertest\": \"^2.0.16\",\n    \"@types/swagger-jsdoc\": \"^6.0.4\",\n    \"@types/swagger-ui-express\": \"^4.1.6\",\n    \"@types/uuid\": \"^9.0.8\",\n    \"@types/ws\": \"^8.5.10\",\n    \"@typescript-eslint/eslint-plugin\": \"^6.13.2\",\n    \"@typescript-eslint/parser\": \"^6.13.2\",\n    \"better-docs\": \"^2.7.2\",\n    \"docsify-cli\": \"^4.4.4\",\n    \"eslint\": \"^8.55.0\",\n    \"eslint-plugin-security\": \"^2.1.0\",\n    \"jest\": \"^29.7.0\",\n    \"jsdoc\": \"^4.0.2\",\n    \"jsdoc-to-markdown\": \"^8.0.1\",\n    \"nodemon\": \"^3.0.2\",\n    \"supertest\": \"^6.3.3\",\n    \"ts-jest\": \"^29.1.1\",\n    \"ts-node\": \"^10.9.1\",\n    \"typescript\": \"^5.3.3\"\n  }\n}\n